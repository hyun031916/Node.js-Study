<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
   
    <h1> Contact List</h1>
    <table border>
        <tr><td>순번</td><td>이름</td><td>이메일</td><td>전화번호</td><td>도시</td><td>비고</td></tr>
    <% data.forEach(function(list){ %>
        <tr><td><%= list.id %></td><td><%= list.name %></td><td><%= list.email %></td><td><%= list.tel %></td><td><%= list.city %></td><td><a href="/update/<%=list.id %>">수정</a>&nbsp;<a href="/delete/<%=list.id %>">삭제</a></td></tr>
   <% })%>
    
     
    </table>
</body>
</html>